<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.constraintsController">
   <center>
      <VBox prefHeight="348.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Path Processing">
               <VBox.margin>
                  <Insets top="80.0" />
               </VBox.margin>
            </Label>
            <Separator prefWidth="200.0" />
            <GridPane prefHeight="68.0" prefWidth="500.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Number Of Tables">
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#constraintmatch" text="Submit" GridPane.columnIndex="2" />
                  <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
                     <children>
                        <Label fx:id="lbl" text="Invalid Number Of Tables" textFill="#f51414" visible="false">
                           <VBox.margin>
                              <Insets top="7.0" />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="nbrtbl">
                           <VBox.margin>
                              <Insets right="7.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                  </VBox>
               </children>
            </GridPane>
         </children>
      </VBox>
   </center>
</BorderPane>
